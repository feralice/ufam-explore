{
  "expo": {
    "name": "ufam-explore",
    "slug": "ufam-explore",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.feralice.frontend"
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "28c6507f-c0e8-45e5-a439-68770472baae"
      }
    },
    "owner": "feralice"
  }
}
